{
  "hosting": {
    "site": "github-b91ab",
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/events/**",
        "function": { "functionId": "ogEvents", "codebase": "qh", "region": "us-central1" }
      },
      {
        "source": "/projects/**",
        "function": { "functionId": "ogEvents", "codebase": "qh", "region": "us-central1" }
      },
      { "source": "**", "destination": "/index.html" }
    ]
  },
  "functions": [
    { "source": "qh", "codebase": "qh" }
  ],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}
